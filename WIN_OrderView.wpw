  procedures :
   -
     name : Notification
     procedure_id : 1154872725491482583
     type_code : 14
     code : |1+
      // Summary: To be called when an update must be done in the GUI
      
      procedure Notification(nNotif, nNotifParam)
      
      if nNotif = NOTIF_ORDER_MODIF _and_ nNotifParam = gnOrderID then
      	Unsubscription(MyWindow..Name,NOTIF_ORDER_MODIF,gnOrderID)
      	DisplayOrder()
      end
     type : 458752
   -
     name : DisplayOrder
     procedure_id : 1154872725491548119
     type_code : 14
     code : |1-
      // Summary: Display the total information
      procedure DisplayOrder()
      
      switch ORDERS.OrderStatus
      	case STATUS_ESTIMATE
      		MyWindow..Title = "Estimate"
      		STC_N_of_xxx = StringBuild("Estimate #: %1", ORDERS.OrderNum)
      		OPT_Modify..Visible = True
      		
      	case STATUS_ORDER_FORM
      		MyWindow..Title = "Order form"
      		STC_N_of_xxx = StringBuild("Order Form #: %1", ORDERS.OrderNum)
      		OPT_Modify..Visible = True
      		
      	case STATUS_INVOICE
      		MyWindow..Title = "Invoice"
      		STC_N_of_xxx = StringBuild("Invoice #: %1", ORDERS.OrderNum)
      		OPT_Modify..Visible = False
      		
      	case STATUS_ORDER_CANCELED
      		MyWindow..Title = "Order canceled"
      		STC_N_of_xxx = StringBuild("Order Form #: %1", ORDERS.OrderNum)
      		OPT_Modify..Visible = False
      
      end
      
      //Information regarding the customer: information about the invoicing, the delivery
      STC_Invoicing_info = CustomerInfoFormat(ORDERS.CustomerNum)
      if ORDERS.CustomerNumDelivery=Nil_UUID then
      	STC_Delivery_info = STC_Invoicing_info
      else
      	STC_Delivery_info = CustomerInfoFormat(ORDERS.CustomerNumDelivery)
      end
      STC_DeliveryInfo = ORDERS.DeliveryRemarks
      
      
      //Total amount of order
      STC_Amount = StringBuild("Amount: %1 IOT (%2 BT)", CurrencyFormat(ORDERS.TotalIOT), CurrencyFormat(ORDERS.TotalBT))
      
      
      //Order lines: the product with their quantity, their price
      mlzDeleteAll(MZ_Details)
      for each ORDLINE where OrderNum = gnOrderID
      	nLineNum is int = mlzAddLine(MZ_Details)
      	MZ_Details[nLineNum].STC_ProductLine = ORDLINE.ProdDesc
      	MZ_Details[nLineNum].STC_QuantityLine = ORDLINE.Quantity
      	HReadSeekFirst(PRODUCT,Reference,ORDLINE.Reference)
      	if HFound() then
      		MZ_Details[nLineNum].STC_LineUP = CurrencyFormat(PRODUCT.PriceBT)
      	end
      	MZ_Details[nLineNum].STC_LineTP = CurrencyFormat(ORDLINE.SalesPrice)
      end
      
      if InAndroidMode() then
      	OPT_Print..Visible = False
      end
      
      //Specific options are enabled according to the order status
      switch ORDERS.OrderStatus
      	case STATUS_ESTIMATE
      		OPT_Action1..Visible = True
      		OPT_Action1..Caption = "Change into Order Form"
      		gnStatusAction1 = STATUS_ORDER_FORM
      		OPT_Action2..Visible = True
      		OPT_Action2..Caption = "Change into Invoice"
      		gnStatusAction2 = STATUS_INVOICE
      		
      	case STATUS_ORDER_FORM
      		OPT_Action1..Visible = True
      		OPT_Action1..Caption = "Change into Invoice"
      		gnStatusAction1 = STATUS_INVOICE
      		OPT_Action2..Visible = True
      		OPT_Action2..Caption = "Change into Canceled Order"
      		gnStatusAction2 = STATUS_ORDER_CANCELED
      		
      	case STATUS_INVOICE,STATUS_ORDER_CANCELED
      		OPT_Action1..Visible = False
      		OPT_Action2..Visible = False
      end
