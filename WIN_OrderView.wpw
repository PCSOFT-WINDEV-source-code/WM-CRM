   -
     code : |1+
      procedure WIN_OrderView( gnOrderID is int )
      
      gnStatusAction1 is int
      gnStatusAction2 is int
      
      //Finds in the database the order that we want to display
      HReadSeekFirst(ORDERS,OrderNum,gnOrderID)
      if HFound(ORDERS) = False then
      	Close()
      end
      
      //And displays this order
      DisplayOrder()
