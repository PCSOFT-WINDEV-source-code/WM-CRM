      if MyChildWindow..Name = "WIN_EditOrder" then
      	
      	//Runs the update code for IW_Order, the list of orders
      	ExecuteProcess(WIN_Manage, trtRequestRefresh) 
      	
      	//If the order was deleted, we can directly close this window
      	//there is no need to view an order that was just deleted
      	HReadSeekFirst(ORDERS,OrderNum,gnOrderID)
      	if HFound(ORDERS) = False then
      		Close()
      	end
      	
      	
      end
