  procedures :
   -
     name : RefreshLPR
     procedure_id : 1154872751262013099
     type_code : 14
     code : |1+
      // Summary: Refreshes the list of contacts
      procedure RefreshLPR()
      
      // Variable
      sSearchKey     is string
      nSelection        is int = -1
      
      // Re-initializes
      LooperDeleteAll(LOOP_CONTACTS)
      
      // Query to retrieve the contacts according to the filters
      switch gnSortOrderContact
      	case 1 // Name
      		QRY_Contacts.pSortItem = "CustomerName"
      		EDT_SEARCH_BY_NAME..Hint = "search by name"
      		
      	case 2 // Company
      		QRY_Contacts.pSortItem = "Company"
      		EDT_SEARCH_BY_NAME..Hint = "search by company"
      		
      	case 3 // City
      		QRY_Contacts.pSortItem = "City"
      		EDT_SEARCH_BY_NAME..Hint = "search by city"
      		
      	case 4 // Type
      		QRY_Contacts.pSortItem = "Country"
      		EDT_SEARCH_BY_NAME..Hint = "search by country"
      end
      
      EDT_SEARCH_BY_NAME = ""
      
      // Saves the search key
      sSearchKey = QRY_Contacts.pSortItem
      
      // Runs the query
      if HExecuteQuery(QRY_Contacts) then
      	CONTACT_FillLPR()
      end
     type : 458752
   -
     name : CONTACT_FillLPR
     procedure_id : 1154872751262078635
     type_code : 14
     code : |1-
      // Summary: Fills the looper of contacts according to the filter passed in parameter
      procedure CONTACT_FillLPR( sFilterContainsKey is string = "" )
      
      nSubscript           is int
      sKeyValue        is string
      sOtherValue      is string
      
      
      // Fills the looper
      for each QRY_Contacts 
      	
      	// Defines the search key
      	switch gnSortOrderContact
      		case 1 // Name
      			sKeyValue   = QRY_Contacts.CustomerName + " " + QRY_Contacts.CustomerFirstName
      			sOtherValue = QRY_Contacts.Company
      			
      		case 2 // Company
      			sKeyValue   = QRY_Contacts.Company
      			sOtherValue = QRY_Contacts.CustomerName + " " + QRY_Contacts.CustomerFirstName
      			
      		case 3 // City
      			sKeyValue   = QRY_Contacts.City
      			sOtherValue = QRY_Contacts.CustomerName + " " + QRY_Contacts.CustomerFirstName
      			if QRY_Contacts.Company<>"" then sOtherValue += " ("+QRY_Contacts.Company+")"
      			
      		case 4 // Country
      			sKeyValue = QRY_Contacts.Country
      			sOtherValue = QRY_Contacts.CustomerName + " " + QRY_Contacts.CustomerFirstName
      			if QRY_Contacts.Company<>"" then sOtherValue += " ("+QRY_Contacts.Company+")"
      			
      	end
      	
      	//If a search was specified by the user, check whether the results respect the filter of the user,
      	//if yes, it can be displayed,
      	//otherwise, no
      	bAddLine is boolean = True
      	if sFilterContainsKey <> "" then
      		if Position(sKeyValue,sFilterContainsKey,1,IgnoreCase)=0 then
      			bAddLine = False
      		end
      	end
      	
      	if bAddLine then
      		
      		// Inserts a row into the looper
      		nSubscript = LooperAddLine(LOOP_CONTACTS, QRY_Contacts.CustomerNum, sKeyValue, sOtherValue,
      		QRY_Contacts.City + " / " + QRY_Contacts.Country, QRY_Contacts.EmailAddress, sKeyValue[[1]])
      		
      	end
      end
     type : 458752
