 message_bar :
  internal_properties : CAAAAAgAAADFL8OS4pvpqk06TsZYLmxGI7mz3EvjBdhwjCItulsjTNfY+9gv1zlraOacfikK9UAoY8W6CHHlMj3C9YKoJTY7bPblhl4LeB4kQ/rsyKyXvH2qhGi9BSu+hfiZAg==
 code_elements :
  internal_properties : CAAAAAgAAACvhiIr8gSQ5loUp83uTso9CuLc4dExenO6nYq0wt6wNhhy8DIKk+8xrI7GMink5ce8SLehbaZBjJVyvy60Arg=
  type_code : 1
  p_codes :
   -
     code : |1+
      procedure WIN_EditContact(gCustomerID is UUID = Nil_UUID)
   -
     code : |1+
      //If a customer identifier was passed in parameter, we are in modification mode,
      //display information about this customer
      
      if gCustomerID <> Nil_UUID  then
      	
      	//Make the Delete button visible
      	OPT_Delete..Visible = True
      	
      	//Find in the database the customer whose identifier is passed in parameter to the window
      	HReadSeekFirst(CUSTOMER,CustomerNum,gCustomerID)
      	
      	//If he is actually found in the database
      	if HFound(CUSTOMER) then
      		
      		//Display his personal details
      		FileToScreen()
      		
      		// Customer radio button
      		nCustomerTypeValue is int = CUSTOMER.Type
      		
      		//Optimization tip: _A_ MZ_RadioButton..Occurrence 
      		// >> "_A_" instead of "A" is used to specify that MZ_RadioButton..Occurrence will be evaluated once only at the beginning of the browse
      		for n = 1 _to_ MZ_ProspectType..Count
      		
      			//Used to make the checkmark visible on the row that must be selected
      			//On this row only
      			if n = nCustomerTypeValue then
      				MZ_ProspectType[n].IMG_Option..Visible = True 
      			else
      				MZ_ProspectType[n].IMG_Option..Visible = False 
      			end
      						
