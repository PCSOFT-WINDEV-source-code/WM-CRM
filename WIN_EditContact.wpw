 menu :
  name : _Menu
  identifier : 0x2006ee994afde654
  internal_properties : 
  options :
   -
     name : OPT_Validate
     identifier : 0x2006ee994afee654
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //Validate the modifications
          
          //In creation mode of a new contact, start to erase the content of the items in memory 
          if gCustomerID = Nil_UUID then
          	HReset(CUSTOMER)
          end
          
          //Transfer the content of controls into the items in memory
          //This applies to the controls that are linked, from their description window, to a file item of the database
          ScreenToFile()
          
          
          //For the other controls not linked from their description window, because a specific process is required for example
          //it is done by programming >>
          
          // Customer radio button
          // Title radio button
          for n = 1 _to_ MZ_ProspectType..Count
          	
          	//Used to make the checkmark visible on the row that must be selected
          	//On this row only
          	if MZ_ProspectType[n].IMG_Option..Visible = True then
          		CUSTOMER.Type = n		
          	end
          	
          end
          
          // Title radio button
          for n = 1 _to_ MZ_CivilStatus..Count
          	
          	//Used to make the checkmark visible on the row that must be selected
          	//On this row only
          	if MZ_CivilStatus[n].IMG_Option..Visible = True then
          		CUSTOMER.Title = n		
          	end
          end
          
          
          //finally: save in the database
          if HSave(CUSTOMER) = True then
          	//Ask to refresh the GUI
          	Notify(NOTIF_CONTACT_MODIF,CUSTOMER.CustomerNum)
          	Close()
          end
          
         type : 32
   -
     name : OPT_Delete
     identifier : 0x2006ee994affe654
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          //Delete the contact
          if YesNo("Do you want to delete this contact?")=Yes then
          	//in the database
          	HDelete()
          	//And during the display  
          	Notify(NOTIF_CONTACT_MODIF,gCustomerID)
          	Close()
          end
         type : 32
   -
     name : OPT_Import
     identifier : 0x2006ee994b00e654
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          if InSimulatorMode() or IniOSEmulatorMode() _or_ InAndroidEmulatorMode() then
          	Info("Selecting contacts from the contacts found on the device operates on the device only.")
          	return
          end
          
          // Selects the contact
          if ContactSelect() = True then
          	
          	EDT_NAME = mContact.Name
          	EDT_PHONE = mContact.OfficePhone
          	EDT_FAX = mContact.OfficeFax
          	EDT_CELLPHONE = mContact.MobilePhone
          	EDT_EMAIL = mContact.Email
          	EDT_COUNTRY = mContact.OfficeCountry
          	EDT_CITY = mContact.OfficeCity
          	EDT_ZC = mContact.OfficeZipCode
          	EDT_ADDRESS = mContact.OfficeAddress
          	EDT_FIRSTNAME = mContact.FirstName
          	EDT_COMPANY = mContact.Company
          	
          end
          
         type : 32
  code_elements :
   type_code : 26
   p_codes : []
