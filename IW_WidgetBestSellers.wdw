     name : RefreshTop10Products
     procedure_id : 1154872798508040752
     type_code : 14
     code : |1-
      // Summary: Refreshes the 10 best sellers
      procedure RefreshTop10Products()
      
      //Delete the previous data from the chart
      grDeleteSeries(CHART_BestProduct,1, grData)
      
      <compile if TypeConfiguration=iOS>
      	grTooltip(CHART_BestProduct,grTooltipFormat, "Ref: " + "[%CATEGORY%]" + CR + "Quantity: " + "[%VALUE%]" ) 
      <end>
      
      //Then browse the query to add the data into the chart
      nSubscript is int
      for each QRY_10BestSellers FromEnd
      	
      	nSubscript++
      	grCategoryLabel(CHART_BestProduct,nSubscript,QRY_10BestSellers.Reference)
      	grAddData(CHART_BestProduct,1, nSubscript, QRY_10BestSellers.TotalQty)	
      	
      end
      
      grDraw(CHART_BestProduct)
