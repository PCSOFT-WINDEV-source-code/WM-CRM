   -
     name : RefreshTurnover
     procedure_id : 1154872785622874126
     type_code : 14
     code : |1-
      // Summary: Refreshes the turnover
      procedure RefreshTurnover()
      
      //Delete the previous data from the chart
      grDeleteSeries(CHART_TO,1,grData)
      
      grSeriesColor(CHART_TO,1,RGB(163,0,72))
      
      <compile if TypeConfiguration=iOS>
      	grTooltip(CHART_TO,grTooltipFormat, "[%CATEGORY%]" + CR + "[%VALUE%]" + " K€" ) 
      <end>
      
      //Then browse the query to add the data into the chart
      nSubscript is int
      for each QRY_CalculateDailyTOOnPeriod 
      	
      	nSubscript ++
      	
      	grCategoryLabel(CHART_TO,nSubscript,DateToString(QRY_CalculateDailyTOOnPeriod.OrdersDate, "DD MMM"))
      	
      	//Divided by 1000 because TO expressed in K€
      	grAddData(CHART_TO, 1, nSubscript , Round(QRY_CalculateDailyTOOnPeriod.the_sum_TotalBT / 1000))
      	
      	
      end
      
      grDraw(CHART_TO)
