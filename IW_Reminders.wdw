  procedures :
   -
     name : DisplayReminders
     procedure_id : 1154872764147376692
     type_code : 14
     code : |1+
      // Summary: Displays the plane of reminders
      procedure DisplayReminders()
      
      LooperDeleteAll(LOOP_PENDING_REMINDER)
      
      //Display the reminders to perform
      for each REMINDER on ReminderDate 
      	
      	if REMINDER.ContributorID = gnContributorID then
      		
      		//With the information about the customer affected by the reminder
      		sCustomerName is string = "Customer not found"
      		HReadSeekFirst(CUSTOMER, CustomerNum, REMINDER.CustomerNum)
      		if HFound() then
      			sCustomerName = StringBuild("%1 %2 %3", FormatCivilStatus(CUSTOMER.Title), CUSTOMER.CustomerName, CUSTOMER.CustomerFirstName)
      		end
      		
      		
      		LooperAddLine(LOOP_PENDING_REMINDER,REMINDER.REMINDERID,
      		DateToString(REMINDER.ReminderDate,"Dddd DD Mmmm YYYY"),
      		REMINDER.ReminderTime,
      		sCustomerName + [CR] + REMINDER.ReminderDetails,
      		REMINDER.bOpened ? "Pending" else "Closed")
      		
      	end
      end
      
      
      //Display "no reminder" if the LPR is empty 
      STC_NoReminder..Visible = LOOP_PENDING_REMINDER..Empty
      
     type : 458752
