     name : DisplayView
     procedure_id : 1154872738376780640
     type_code : 14
     code : |1-
      procedure DisplayView()
      
      OPT_Refresh..Visible = False
      OPT_New_contact..Visible = False
      OPT_New_order..Visible = False
      OPT_New_reminder..Visible = False
      
      switch ACTB_ActionBar
      	case 1 // DASH
      		MyWindow..Title = "Dashboard"
      		OPT_Refresh..Visible = True
      		
      	case 2 // Customers
      		MyWindow..Title = "Customers"
      		OPT_New_contact..Visible = True
      		
      	case 3 // Reminder
      		MyWindow..Title = "Reminder"
      		OPT_New_reminder..Visible = True
      		
      	case 4 // Orders
      		MyWindow..Title = "Orders"
      		OPT_New_order..Visible = True
      end
