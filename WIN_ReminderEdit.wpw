 menu :
  name : _Menu
  identifier : 0x2006eeab50332d6f
  internal_properties : 
  options :
   -
     name : OPT_Validate
     identifier : 0x2006eeab50342d6f
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //A customer must necessarily be associated with the reminder
          if gCustomerID = Nil_UUID then
          	Error("A customer must be associated with the reminder")
          	return
          end
          
          
          //If we are currently creating a new reminder, clear the memory items from the reminder data file beforehand 
          if gnReminderID = -1 then
          	HReset(REMINDER)
          end
          
          //Then, transfer the data from the screen to the memory items
          //This applies to the controls that are linked to a file item from the description window of the control
          ScreenToFile()
          
          //And for the other ones, we 
          REMINDER.CustomerNum = gCustomerID
          REMINDER.ContributorID = CONTRIBUTOR.ContributorID
          REMINDER.bOpened = True
          
          //Save in the database
          HSave(REMINDER)
          
          //Notify the window that the reminder was modified
          Notify(NOTIF_REMINDER_MODIF,REMINDER.REMINDERID)
          
          Close()
         type : 32
   -
     name : OPT_Close
     identifier : 0x2006eeab50352d6f
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //Ask to close the reminder
          if YesNo(<§$0000§> ) = Yes then
          	
          	//Find it in database
          	HReadSeekFirst(REMINDER,REMINDERID,gnReminderID)
          	
          	if HFound() then
          		//If it is found, modify it in database
          		REMINDER.bOpened = False
          		HModify(REMINDER)
          		//And notify of the modification
          		Notify(NOTIF_REMINDER_MODIF,gnReminderID)
          		Close()
          	end
          	
          end
         type : 32
   -
     name : OPT_Delete
     identifier : 0x2006eeab50362d6f
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          //Ask to delete the reminder
          if YesNo("Do you want to delete this reminder?") = Yes then
          	
          	//Find it in database 
          	HReadSeekFirst(REMINDER,REMINDERID,gnReminderID)
          	
          	if HFound() then
          		//If it is found, delete it from the database
          		HDelete(REMINDER)
          		//And notify of this deletion
          		Notify(NOTIF_REMINDER_MODIF,gnReminderID)
          		Close()
          	end
          	
          end
         type : 32
