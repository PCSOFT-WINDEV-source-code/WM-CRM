  type_code : 1
  p_codes :
   -
     code : |1+
      procedure WIN_ReminderEdit(gnReminderID is int = -1)
      
      gCustomerID is UUID = Nil_UUID
   -
     code : |1+
      //Either we want to create a new reminder, or we want to display an existing one
      
      //If the window received a reminder identifier, that's what we want to display
      if gnReminderID <> -1 then
      	
      	//Find it in the database
      	HReadSeekFirst(REMINDER,REMINDERID,gnReminderID)
      	//If it is actually found, display it in the window
      	if HFound() then
      		
      		FileToScreen()
      		
      		//And display information about the customer affected by this reminder
      		gCustomerID = REMINDER.CustomerNum
      		HReadSeekFirst(CUSTOMER,CustomerNum,REMINDER.CustomerNum)
      		if HFound() then
      			MZ_ReminderInfo.STC_CustomerName = StringBuild("%1 %2 %3", FormatCivilStatus(CUSTOMER.Title),CUSTOMER.CustomerName,CUSTOMER.CustomerFirstName)
      		end
      		
      	end
      	
      else
      	
      	//We want to create a new reminder, define some information beforehand
      	EDT_Reminder_date = Today()
      	tTime is time
      	tTime..Minute = 0
      	tTime..Hour += 2
      	EDT_Reminder_time = tTime
      	
      end
     type : 34
