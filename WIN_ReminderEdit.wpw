  procedures :
   -
     name : Notification
     procedure_id : 1154872772737707375
     type_code : 14
     code : |1-
      // Summary: Indicates to the window that the customer linked to this reminder was modified, therefore the window must be updated
      procedure Notification( nNotif , nNotifParam )
      
      switch nNotif
      	case NOTIF_CUSTOMER_SELECTION
      		
      		//Retrieve the new linked customer and display his information 
      		gCustomerID = nNotifParam
      		HReadSeekFirst(CUSTOMER,CustomerNum,gCustomerID)
      		if HFound() then
      			
      			MZ_ReminderInfo.STC_CustomerName = StringBuild("%1 %2 %3",FormatCivilStatus(CUSTOMER.Title),CUSTOMER.CustomerName,CUSTOMER.CustomerFirstName)
      			Unsubscription(MyWindow..Name,NOTIF_CUSTOMER_SELECTION,"*")
      		end
      		
      	other case		
      end
     type : 458752
