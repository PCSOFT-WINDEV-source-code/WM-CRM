 menu :
  name : _Menu
  identifier : 0x2006ee9e4cdef9bd
  internal_properties : 
  options :
   -
     name : OPT_Validate
     identifier : 0x2006ee9e4cdff9bd
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          if gnOrderID = -1 then
          	
          	HReset(ORDERS)
          	
          	
          	//Defines the type of commercial actions: estimate, order form, invoice..
          	for n = 1 _to_ MZ_OrderType..Count
          		//Used to make the checkmark visible on the row that must be selected
          		//On this row only
          		if MZ_OrderType[n].IMG_Option..Visible = True then
          			ORDERS.OrderStatus = n		
          		end
          	end
          	
          	ORDERS.OrdersDate = DateSys()
          	
          end
          
          //Defines the payment mode: wire, check, credit card
          for n = 1 _to_ MZ_PaymentMode..Count
          	//Used to make the checkmark visible on the row that must be selected
          	//On this row only
          	if MZ_PaymentMode[n].IMG_Option2..Visible = True then
          		ORDERS.PaymentMode = n		
          	end
          end
          
          ORDERS.CustomerNum = gnCustomerNumInvoicing
          ORDERS.CustomerNumDelivery = gnCustomerNumDelivery
          ORDERS.DeliveryRemarks = EDT_DeliveryRmk
          
          cyTotalBT, cyTotalVAT are currencies
          
          //Modify or add the order in the database
          if HSave(ORDERS) = True then 
          	
          	//If the backup in the database is successful .. 
          	
          	// Update the order lines
          	HExecuteQuery(QRY_DeleteOrderLine,hQueryDefault,ORDERS.OrderNum)
          	
          	ORDLINE.OrderNum = ORDERS.OrderNum
          	ORDLINE.Delivered = False
          	
          	for each stLine of garrOrderLine
          		
          		HReadSeekFirst(PRODUCT,Reference,stLine.sReference)
          		if HFound() then
          			ORDLINE.ProdDesc = PRODUCT.ProdDesc
          			ORDLINE.Quantity = stLine.nQuantity
          			ORDLINE.SalesPrice = PRODUCT.PriceBT * ORDLINE.Quantity
          			ORDLINE.Reference = PRODUCT.Reference
          			ORDLINE.Discount = 0
          			ORDLINE.VATRate = PRODUCT.VATRate
          			HAdd(ORDLINE)
          			
          			cyTotalBT  += ORDLINE.SalesPrice
          			cyTotalVAT += (ORDLINE.SalesPrice*PRODUCT.VATRate)/100
          			
          		end
          	end
          	
          	// Update the totals in the ORDERS file
          	ORDERS.TotalBT = cyTotalBT
          	ORDERS.TotalVAT = cyTotalVAT
          	ORDERS.TotalIOT = cyTotalBT + cyTotalVAT
          	
          	HModify(ORDERS)
          	
          	// Notifies the subscribers about the order modification
          	Notify(NOTIF_ORDER_MODIF,ORDERS.OrderNum)
          end
          
          Close()
          
         type : 32
   -
