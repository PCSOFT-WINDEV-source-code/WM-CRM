      // Summary: Refreshes the customer information in the display of the window
      procedure RefreshCustomerInfo()
      
      MZ_Invoicing.STC_Text = CustomerInfoFormat(gnCustomerNumInvoicing)
      MZ_Delivery.STC_DeliveryText = CustomerInfoFormat(gnCustomerNumDelivery = Nil_UUID ? gnCustomerNumInvoicing else gnCustomerNumDelivery)
     type : 458752
   -
     name : RefreshContentLPR
     procedure_id : 1154872716901284285
     type_code : 14
     code : |1+
      // Summary: Refreshes the list of order products in the display of the window
      procedure RefreshContentLPR()
      
      //First, erase all
      mlzDeleteAll(MZ_Products)
      
      //then, add all again 
      stLine is STOrderLine
      for each stLine of garrOrderLine
      	nLine is int = mlzAddLine(MZ_Products)
      	MZ_Products[nLine].STC_ProdNameLine = stLine.sProdDesc
      	MZ_Products[nLine].EDT_LineQty = stLine.nQuantity
      end
