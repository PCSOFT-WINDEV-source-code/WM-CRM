     code : |1-
      // Summary: To be called when an update must be done in the GUI, further to the selection of a customer or product 
      
      procedure Notification(nNotif, nNotifParam)
      
      switch nNotif
      	case NOTIF_CUSTOMER_SELECTION
      		if gbSelInvoicingContact then
      			gnCustomerNumInvoicing = nNotifParam
      		else
      			gnCustomerNumDelivery = nNotifParam
      		end
      		RefreshCustomerInfo()
      		Unsubscription(MyWindow..Name,NOTIF_CUSTOMER_SELECTION,"*")
      		
      	case NOTIF_PRODUCT_SELECTION
      		HReadSeekFirst(PRODUCT,Reference,nNotifParam)
      		if HFound() then
      			nProdRank is int = ArraySeek(garrOrderLine,asLinear,"sReference",PRODUCT.Reference)
      			if nProdRank>0 then
      				garrOrderLine[nProdRank].nQuantity++
      			else
      				stLine is STOrderLine
      				stLine.nQuantity = 1
      				stLine.sProdDesc = PRODUCT.ProdDesc
      				stLine.sReference = PRODUCT.Reference
      				ArrayAdd(garrOrderLine,stLine)
      			end
      			RefreshContentLPR()
      		end
      		Unsubscription(MyWindow..Name,NOTIF_PRODUCT_SELECTION,"*")
      end
