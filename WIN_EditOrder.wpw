     identifier : 0x2006ee9e4ce0f9bd
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1+
          if YesNo(StringBuild("Do you want to delete this %1",ORDERS.OrderType=1 ? "Estimate" else "Order Form"))=Yes then
          	HReadSeekFirst(ORDERS,OrderNum,gnOrderID)
          	if HFound() then
          		HDelete(ORDERS)
          		Close()
          	end
          end
