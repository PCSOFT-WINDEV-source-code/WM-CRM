  internal_properties : CAAAAAgAAACvhiIr8gSQ5loUp83uTso9CuLc4dExenO6nYq0wt6wNhhy8DIKk+8xrI7GMink5ce8SLehbaZBjJVyvy60Arg=
  type_code : 1
  p_codes :
   -
     code : |1-
      procedure WIN_EditOrder(gnOrderID is int = -1)
      
      gnCustomerNumInvoicing is UUID = Nil_UUID
      gnCustomerNumDelivery is UUID = Nil_UUID
      gbSelInvoicingContact is boolean
      
      STOrderLine is Structure
      	sReference is string
      	sProdDesc is string
      	nQuantity is int
      end
      
      garrOrderLine is array of STOrderLine
   -
     code : |1-
      
      if gnOrderID = -1 then
      	
      	// Creation
      	
      	nLine is int 
      	nLine = mlzAddLine(MZ_OrderType)
      	MZ_OrderType[nLine].STC_CreateWhat = "Estimate"
      	
      	
      	nLine = mlzAddLine(MZ_OrderType)
      	MZ_OrderType[nLine].STC_CreateWhat = "Order form"
      	
      	nValue is int = 1
      	
      	//Optimization tip: _A_ MZ_RadioButton..Occurrence 
      	// >> "_A_" instead of "A" is used to specify that MZ_RadioButton..Occurrence will be evaluated once only at the beginning of the browse
      	for n = 1 _to_ MZ_OrderType..Count
      		
      		//Used to make the checkmark visible on the row that must be selected
      		//On this row only
      		if n = nValue then
      			MZ_OrderType[n].IMG_Option..Visible = True 
      		else
      			MZ_OrderType[n].IMG_Option..Visible = False 
      		end
      		
      	end
      	
      	OPT_Delete..Visible = False
      	
      else
      	
      	// Edit
      	
      	OPT_Delete..Visible = True
      	
      	HReadSeekFirst(ORDERS,OrderNum,gnOrderID)
      	if HFound(ORDERS) = False then
      		Close()
      	end
      	
      	STC_CreateWhat = "Type of commercial action" 
      			
      	nLineComActionType is int 
      	nLineComActionType = mlzAddLine(MZ_OrderType)
      	
      	
      	switch ORDERS.OrderStatus
      		case STATUS_ESTIMATE
      			MZ_OrderType[nLineComActionType].STC_CreateWhat =  "Estimate"
      			
      		case STATUS_ORDER_FORM
      			MZ_OrderType[nLineComActionType].STC_CreateWhat =  "Order form"
      			
      		case STATUS_INVOICE
      			MZ_OrderType[nLineComActionType].STC_CreateWhat =  "Invoice"
      			
      		case STATUS_ORDER_CANCELED
      			MZ_OrderType[nLineComActionType].STC_CreateWhat = "Order canceled"
      			
      	end
      	
      	
      	
      
      	//Payment mode
      	nPaymentValue is int  = ORDERS.PaymentMode
      	
      	//Optimization tip: _A_ MZ_RadioButton..Occurrence 
      	// >> "_A_" instead of "A" is used to specify that MZ_RadioButton..Occurrence will be evaluated once only at the beginning of the browse
      	for n = 1 _to_ MZ_PaymentMode..Count
      		
      		//Used to make the checkmark visible on the row that must be selected
      		//On this row only
      		if n = nPaymentValue then
      			MZ_PaymentMode[n].IMG_Option2..Visible = True 
      		else
      			MZ_PaymentMode[n].IMG_Option2..Visible = False 
      		end
      		
      	end
      	
      	
      	// Customer, invoicing and delivery
      	gnCustomerNumInvoicing = ORDERS.CustomerNum
      	gnCustomerNumDelivery = ORDERS.CustomerNumDelivery
      	RefreshCustomerInfo()
      		
      	// Initialize the array of order lines
      	for each ORDLINE where OrderNum = gnOrderID
      		stLine is STOrderLine
      		stLine.sReference = ORDLINE.Reference
      		stLine.sProdDesc = ORDLINE.ProdDesc
      		stLine.nQuantity = ORDLINE.Quantity
      		ArrayAdd(garrOrderLine,stLine)
      	end
      	RefreshContentLPR()
      	
      	EDT_DeliveryRmk = ORDERS.DeliveryRemarks
      end
     type : 34
